<?xml version="1.0" encoding="UTF-8"?>
<root>
  <material>
    <description>Matte plastic that is recolorable using the User 1 custom defined color, with a custom bump map.</description>
    <preview>textures/outdoor_speaker_bump.jpg</preview>
    <renderpass>
      <shadercore>Result = Mul(Entity1, 0.7);
Specular = 0;
Roughness = 1;</shadercore>
      <transparency mode="opaque"/>
      <linearshading/>
      <rgbgen mode="bumplighting"/>
      <texunit>
        <map>textures/outdoor_speaker_normal.jpg</map>
        <bump mode="normalgloss"/>
        <tcgen mode="base"/>
      </texunit>
    </renderpass>
  </material>
</root>